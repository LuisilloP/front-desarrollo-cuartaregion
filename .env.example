# Strapi base URL (e.g. http://localhost:1337)
STRAPI_URL=b

# Strapi API token (optional if Strapi is public)
STRAPI_API_TOKEN=a

# Cases endpoint if API ID is localized (e.g. /api/casos)
STRAPI_CASES_ENDPOINT=/api/casos

# Force uso de mocks (ignora Strapi). Pon en "true" para build/demo sin backend.
USE_MOCK_DATA=true
